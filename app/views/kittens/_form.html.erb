<%= form_for @kitten do |f| %>

  <%= f.label :name, "Name: " %>
  <%= f.text_field :name %>

  <%= f.label :age, "Age: " %>
  <%= f.number_field :age %>

  <%= f.label :cuteness, "Cuteness Level: " %>
  <%= f.number_field :cuteness %>

  <%= f.label :softness, "Softness Level: " %>
  <%= f.number_field :softness %>

  <%= f.submit %>
<% end %>
